<config xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
	xmlns='http://www.ehcache.org/v3'
	xsi:schemaLocation="
        http://www.ehcache.org/v3 http://www.ehcache.org/schema/ehcache-core-3.0.xsd">

	<cache alias="palConfigListEhCache">
		<expiry>
			<ttl unit="seconds">3600</ttl>
		</expiry>
		<heap unit="entries">1000</heap>
	</cache>
	<cache alias="palFieldListEhCache">
		<expiry>
			<ttl unit="seconds">3600</ttl>
		</expiry>
		<heap unit="entries">1000</heap>
	</cache>
	<cache alias="palRoleListEhCache">
		<expiry>
			<ttl unit="seconds">3600</ttl>
		</expiry>
		<heap unit="entries">100</heap>
	</cache>
	<cache alias="palTemplateListEhCache">
		<expiry>
			<ttl unit="seconds">3600</ttl>
		</expiry>
		<heap unit="entries">20</heap>
	</cache>
	<cache alias="palUserListEhCache">
		<expiry>
			<ttl unit="seconds">3600</ttl>
		</expiry>
		<heap unit="entries">100</heap>
	</cache>
	<cache alias="palDownloadTemplateEhCache">
		<expiry>
			<ttl unit="seconds">3600</ttl>
		</expiry>
		<heap unit="entries">20</heap>
	</cache>
</config>