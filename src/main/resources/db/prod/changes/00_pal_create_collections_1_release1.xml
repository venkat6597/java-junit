<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

	<changeSet id="create_palrole_collection" author="harish">
		<ext:createCollection collectionName="PALRole" />
	</changeSet>
	<changeSet id="create_palconfiguration_collection" author="harish">
		<ext:createCollection collectionName="PALConfiguration" />
	</changeSet>
	<changeSet id="create_palfields_collection" author="harish">
		<ext:createCollection collectionName="PALFields" />
	</changeSet>
	<changeSet id="create_paltemplate_collection" author="harish">
		<ext:createCollection collectionName="PALTemplate" />
	</changeSet>
	<changeSet id="create_palproduct_collection" author="harish">
		<ext:createCollection collectionName="PALProduct" />
	</changeSet>
	<changeSet id="create_palproject_collection" author="harish">
		<ext:createCollection collectionName="PALProject" />
	</changeSet>
	<changeSet id="create_palauditlogs_collection" author="harish">
		<ext:createCollection collectionName="PALAuditLogs" />
	</changeSet>
</databaseChangeLog>