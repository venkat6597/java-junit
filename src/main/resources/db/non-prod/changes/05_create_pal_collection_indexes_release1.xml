<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

	<changeSet id="create_index_pal_audit_logs"
		author="joice.joy">
		<ext:createIndex collectionName="PALAuditLogs">
			<ext:keys>
				{ productId:1 }
			</ext:keys>
			<ext:options>
				{ name: "productId_Idx" }
			</ext:options>
		</ext:createIndex>
	</changeSet>
	<changeSet id="create_index_pal_product" author="joice.joy">
		<ext:createIndex collectionName="PALProduct">
			<ext:keys>
				{ projectId:1 }
			</ext:keys>
			<ext:options>
				{ name: "projectId_Idx" }
			</ext:options>
		</ext:createIndex>

		<ext:createIndex collectionName="PALProduct">
			<ext:keys>
				{ createdDate:1 }
			</ext:keys>
			<ext:options>
				{ name: "createdDate_Idx" }
			</ext:options>
		</ext:createIndex>
		
		<ext:createIndex collectionName="PALProduct">
			<ext:keys>
				{ "datafields.fieldId": 1, "datafields.fieldValue": 1 }
			</ext:keys>
			<ext:options>
				{ name: "fieldId_fieldValue_Idx" }
			</ext:options>
		</ext:createIndex>
	</changeSet>
	<changeSet id="create_index_pal_template" author="joice.joy">
		<ext:createIndex collectionName="PALTemplate">
			<ext:keys>
				{ templateName:1 }
			</ext:keys>
			<ext:options>
				{ name: "templateName_Idx" }
			</ext:options>
		</ext:createIndex>
	</changeSet>
	<changeSet id="create_index_pal_project" author="joice.joy">
		<ext:createIndex collectionName="PALProject">
			<ext:keys>
				{ createdDate:1 }
			</ext:keys>
			<ext:options>
				{ name: "createdDate_Idx" }
			</ext:options>
		</ext:createIndex>
		<ext:createIndex collectionName="PALProject">
			<ext:keys>
				{ status:1 }
			</ext:keys>
			<ext:options>
				{ name: "status_Idx" }
			</ext:options>
		</ext:createIndex>
		<ext:createIndex collectionName="PALProject">
			<ext:keys>
				{ projectName:1 }
			</ext:keys>
			<ext:options>
				{ name: "projectName_Idx" }
			</ext:options>
		</ext:createIndex>
		<ext:createIndex collectionName="PALProject">
			<ext:keys>
				{ projectType:1 }
			</ext:keys>
			<ext:options>
				{ name: "projectType_Idx" }
			</ext:options>
		</ext:createIndex>
		<ext:createIndex collectionName="PALProject">
			<ext:keys>
				{ financialYear:1 }
			</ext:keys>
			<ext:options>
				{ name: "financialYear_Idx" }
			</ext:options>
		</ext:createIndex>
		<ext:createIndex collectionName="PALProject">
			<ext:keys>
				{ projectCompletionDate:1 }
			</ext:keys>
			<ext:options>
				{ name: "projectCompletionDate_Idx" }
			</ext:options>
		</ext:createIndex>
	</changeSet>
</databaseChangeLog>